mixin products-group(props = {})
  include products-group-name
  include products-list

  - const { counter, counterColor, name, products, prdouctsGroupNameAttrs, productslistAtrrs } = props;

  .products-group&attributes(attributes)
    +products-group-name({counter: counter, counterColor: counterColor, name: name})&attributes(prdouctsGroupNameAttrs)
    +products-list({products: products})&attributes(productslistAtrrs)
